      "type" : "SingleSelect",
      "rule" : "'use strict';\n({params, imports}) => {\n    const programEncounter = params.entity;\n    const moment = imports.moment;\n    const formElement = params.formElement;\n    const _ = imports.lodash;\n    let visibility = true;\n    let value = null;\n    let answersToSkip = [];\n    let answersToShow = [];\n    let validationErrors = [];\n    let gender = programEncounter.individual.gender;\n    let age = programEncounter.individual.getAgeInYears(asOnDate = moment(), precise = true)\n    \n    visibility = gender.isFemale() || (gender.isMale() && age >= 10)\n\n    function getAnswerToShow(HbValues) {\n        let answerList = [];\n        _.forEach(HbValues, (answer) => {\n            const answerToShow = formElement.getAnswerWithConceptUuid(answer);\n            if (answerToShow) answerList.push(answerToShow);\n        });\n        return answerList;\n    }\n    \n    if(visibility) {\n        femaleHbValues = ['f015ffb9-babc-434f-87d1-d46c1a52fa2d', '5b943e5b-56c7-40b4-977d-9c9291fec8b0', '9847a475-3347-49d0-9f67-60a985003f96', '559f614a-1386-4662-ba25-f2cd5017ea50']\n        maleHbValuesAge10to15 = ['f015ffb9-babc-434f-87d1-d46c1a52fa2d', 'f2ce9989-d20c-4fe9-895b-c00ab9b5690e', '2cc1be43-1a35-4cb6-9120-5c114c9e58c2', '559f614a-1386-4662-ba25-f2cd5017ea50']\n        maleHbValuesAgeMoreThan15 = ['f015ffb9-babc-434f-87d1-d46c1a52fa2d', 'f2ce9989-d20c-4fe9-895b-c00ab9b5690e', '18a31a40-5e6d-496c-93e7-d8a47fa45876', 'be8e6b63-d5e8-47af-9efe-2cdfbd84c3ba']\n        \n        if(gender.isFemale()) {\n            answersToShow = getAnswerToShow(femaleHbValues)\n        } else if(gender.isMale() && age > 15) {\n            answersToShow = getAnswerToShow(maleHbValuesAgeMoreThan15)\n        } else if(gender.isMale() && age >= 10) {\n            answersToShow = getAnswerToShow(maleHbValuesAge10to15)\n        } else {\n            visibility = false;\n        }\n    }\n  \n    return new imports.rulesConfig.FormElementStatus(formElement.uuid, visibility, value, answersToSkip, validationErrors, answersToShow);\n};",
